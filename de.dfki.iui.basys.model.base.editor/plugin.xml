<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated base -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="de.dfki.iui.basys.model.base.presentation.BaseModelWizardID"
            name="%_UI_BaseModelWizard_label"
            class="de.dfki.iui.basys.model.base.presentation.BaseModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/BaseModelFile.gif">
         <description>%_UI_BaseModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated base -->
      <editor
            id="de.dfki.iui.basys.model.base.presentation.BaseEditorID"
            name="%_UI_BaseEditor_label"
            icon="icons/full/obj16/BaseModelFile.gif"
            extensions="base"
            class="de.dfki.iui.basys.model.base.presentation.BaseEditor"
            contributorClass="de.dfki.iui.basys.model.base.presentation.BaseActionBarContributor">
      </editor>
   </extension>

</plugin>
