<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="product" nsURI="http://www.dfki.de/iui/basys/model/product"
    nsPrefix="product">
  <eAnnotations source="http://de.dfki.iui.mmds/CoreModel"/>
  <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
    <details key="settingDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL"/>
    <details key="invocationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL"/>
    <details key="validationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL"/>
  </eAnnotations>
  <eClassifiers xsi:type="ecore:EClass" name="ProductCatalogue">
    <eStructuralFeatures xsi:type="ecore:EReference" name="productGroups" upperBound="-1"
        eType="#//ProductGroup" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ProductGroup" eSuperTypes="../../de.dfki.iui.basys.model.base/model/base.ecore#//Entity">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="besser ProductType?"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="productVariants" upperBound="-1"
        eType="#//ProductVariant" containment="true" eOpposite="#//ProductVariant/productGroup"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ProductVariant" eSuperTypes="../../de.dfki.iui.basys.model.base/model/base.ecore#//Entity">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="In BaSys braucht man wohl schon noch einen ProductType, dann sollte die Variante darauf verweisen. Braucht man hier eine BOM?"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="productGroup" eType="#//ProductGroup"
        eOpposite="#//ProductGroup/productVariants"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="bom" lowerBound="1" eType="#//BillOfMaterial"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BillOfMaterial" eSuperTypes="../../de.dfki.iui.basys.model.base/model/base.ecore#//Entity">
    <eStructuralFeatures xsi:type="ecore:EReference" name="entries" upperBound="-1"
        eType="#//BOMEntry" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BOMEntry">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="count" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="material" lowerBound="1"
        eType="ecore:EClass material.ecore#//Material"/>
  </eClassifiers>
</ecore:EPackage>
