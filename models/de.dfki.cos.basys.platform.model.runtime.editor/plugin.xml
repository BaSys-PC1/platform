<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated runtime -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="de.dfki.cos.basys.platform.model.runtime.communication.presentation.CommunicationModelWizardID"
            name="%_UI_CommunicationModelWizard_label"
            class="de.dfki.cos.basys.platform.model.runtime.communication.presentation.CommunicationModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/CommunicationModelFile.gif">
         <description>%_UI_CommunicationModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated runtime -->
      <editor
            id="de.dfki.cos.basys.platform.model.runtime.communication.presentation.CommunicationEditorID"
            name="%_UI_CommunicationEditor_label"
            icon="icons/full/obj16/CommunicationModelFile.gif"
            extensions="communication"
            class="de.dfki.cos.basys.platform.model.runtime.communication.presentation.CommunicationEditor"
            contributorClass="de.dfki.cos.basys.platform.model.runtime.communication.presentation.CommunicationActionBarContributor">
      </editor>
   </extension>

</plugin>
